    "ethers": "^5.7.2",
    "framer-motion": "^8.0.2",
    "next": "^13.1.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-simple-typewriter": "^5.0.1",
    "react-use": "^17.4.0",
